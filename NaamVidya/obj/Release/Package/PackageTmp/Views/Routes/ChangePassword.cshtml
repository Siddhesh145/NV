<div class="business-name-section">
    <div class="container">
        <div class="row mt-5 mb-5">
            <div class="col s12 l9">
                <div class="card z-depth-2 mb-5">
                    <div class=" bg-dark lead">
                        <img src="images/dummy-img.jpg" alt="" class="card-img-top" />
					</div>
                    <b style="color:#d70a61;margin-top:20px;font-size:20px;">Change Password</b>
                    <hr />
                    <br />
                    <form class="order-form form-comflex">
                        <div class="form-group row">
                            <label for="opassword" class="col l12 m12 s12 col-form-label">Existing Password<span style="color:red">*</span></label>
                            <div class="col l12 m12 s12">
                                <input class="form-control" type="password" data-ng-model="OPassword" id="opassword">
							</div>
						</div>
                        <div class="form-group row">
                            <label for="password" class="col l12 m12 s12 col-form-label">New Password<span style="color:red">*</span></label>
                            <div class="col l12 m12 s12">
                                <input class="form-control" type="password" data-ng-model="Password" id="password">
							</div>
						</div>
                        <div class="form-group row">
                            <label for="Cpassword" class="col l12 m12 s12 col-form-label">Confirm Password<span style="color:red">*</span></label>
                            <div class="col l12 m12 s12">
                                <input class="form-control" type="password" data-ng-model="ChangePassword" id="Cpassword">
							</div>
						</div>
                        <div class="form-group row">
                            <label id="errorMsg" class="col l12 m12 s12 col-form-label"><span style="color:red">{{errorMsg}}</span></label>
						</div>
                        <div class="form-group row">
                            <label for="example-email-input" class="col l12 m12 s12 col-form-label"></label>
                            <div class="col l12 m12 s12">
                                <button type="button" data-ng-click="f_Update();" class="waves-effect waves-light btn">Update</button>
							</div>
						</div>
					</form>
				</div>
			</div>
            <div class="col s12 l3" data-ng-controller="sidebarController">
                <div class="bg-yellow animated slideInBottom">
                    <div class="form clearfix">
                        <h5 class="lead wc">
                            <img src="images/soundicon.png" alt="" />Free First<br>
                            Name Analysis
						</h5>
                        <p>
                            <input class="col  s12" type="text" data-ng-model="newfirstName" placeholder="Enter First Name" only-alphabet />
							<label style="color:#ffffff;font-size:14px;text-align:left;">{{errMsg}}</label>    
						</p>
                        <!--p>Date of Birth<br/>
							<input class="col  s12" type="text" />
						</p-->
                        <input type="button" class="waves-effect waves-light pink btn" value="Analyse" data-ng-click="analyzeFirstName()" />
					</div>
				</div>
                <div class="animated slideInBottom bg-aqua" style="margin-bottom:20px;">
                    <div class="list">
                        <ul>
                            <li><a href="/baby-name-gift">Gift Baby Name</a></li>
                            <li><a href="/about-us">About NaamVidya</a></li>
                            <li><a href="/why-names-matter">Why names matter</a></li>
                            <li><a href="/balanced-name">Origins of balanced name</a></li>
                            <li><a href="/ethics-beliefs">Ethics & Beliefs</a></li>
                            <li><a href="/full-name-analysis">Full Name Report</a></li>
                            @*<li><a href="/sku">Order a Name</a></li>*@
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>