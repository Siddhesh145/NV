<script>
    $(document).ready(function () {
        $('.owl-carousel').owlCarousel({
            items: 2,
            autoplay: true,
            autoplayTimeout: 10000,
            rewindNav: true,
            rewindSpeed: 0,
            dots: true,
            margin: 50,
            responsive: {
                0: {
                    items: 1,
				},
                769: {
                    items: 2,
				}
			}
		});
        $('.singlesection h5').matchHeight();
        $("#howitworks").click(function (e) {
            e.preventDefault()
            if ($(".howitworks").hasClass('active')) {
                $(this).removeClass('active');
                $(".howitworks").removeClass('active');
                $(".howitworks").slideUp('slow');
				} else {
                $(this).addClass('active');
                $("#pricing").removeClass('active');
                $(".pricing").removeClass('active');
                $("#firstnameanalysis").removeClass('active');
                $(".firstnameanalysis").removeClass('active');
                $(".pricing").slideUp('slow');
                $(".firstnameanalysis").slideUp('slow');
                $(".howitworks").addClass('active');
                $(".howitworks").slideDown('slow');
                var target = jQuery(this.getAttribute('href'));
                if (target.length) {
                    event.preventDefault();
                    jQuery('html, body').stop().animate({
                        scrollTop: target.offset().top
					}, 1000);
				}
			}
		});
        $("#pricing").click(function (e) {
            e.preventDefault()
            if ($(".pricing").hasClass('active')) {
                $(this).removeClass('active');
                $(".pricing").removeClass('active');
                $(".pricing").slideUp('slow');
				} else {
