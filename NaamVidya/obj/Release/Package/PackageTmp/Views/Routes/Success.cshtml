}
@Html.Raw(ViewBag.output);
@*@model Trag.Models.PayUMoneyViewModel*@
<!DOCTYPE html>
<html  data-ng-app="nvApp">
	<head>
		<title>Know the power of a name | Free first name and full name analysis | Choose a name for baby or correct your name through the ancient science of numbers | Learn Why Names Matter</title>
		<meta NAME="description" Content="A name can bring success or struggle in life. Take a free first name and full name analysis to see how it has affected you or your baby. Understand why a naming ceremony was a ritual and why saints took up a new name on their spiritual journeys." />
		<meta property="og:image" content="https://www.naamvidya.com/images/NVLogo_Small.png">
		<meta name="keywords" content="NaamVidya, Naam Vidya, Baby Name, Adult Name, Child Name, Spouse Name, Name Change, Numerology, Nominative Determinism, Hindu Baby Name, Muslim Baby Name, Name and destiny, Name Analysis, astrology, Chaldean, name correction, changing names, name meaning, consciousness, Purpose of life, Karma, why names matter, how to choose a name, personality, nature, purpose of life." />
		<!--Import Google Icon Font-->
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet">
		<!--Import materialize.css-->
		<link type="text/css" rel="stylesheet" href="~/css/materialize.min.css" media="all" />
		<link type="text/css" rel="stylesheet" href="~/css/font-awesome.min.css" media="all" />
		<link rel="stylesheet" type="text/css" href="http://cdn.jsdelivr.net/jquery.slick/1.6.0/slick.css" />
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
		<link type="text/css" rel="stylesheet" href="~/css/style.css" media="all" />
		<!--Let browser know website is optimized for mobile-->
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
		<!-- <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" /> -->
		<style>
			.home-layout{
			background:#ffffff !important;
			}
			body {
            position: relative;
            background: url('images/bg.png');
			}
			.affix {
            position: fixed;
            z-index: -1;
			}
			.affix {
            top: 0;
			}
            .affix ~ .container {
			position: relative;
			top: 50px;
            }
			.dropdown {
			position: absolute; top: 2px; left: 0.015625px; opacity: 1; display: block;
			}
			.showbody {
			display: block !important;
			}
			.resultName
			{
			padding: 10px;
			margin:0px;
			display: block;   
			clear: both;
			font-size: 36px;
			font-family: 'Kaushan Script', cursive;
			color: #d5a53a;
			text-align:center;
			}
		</style>
		<script>
			$(document).ready(function(){
				sessionStorage.setItem("GetOrderFlag", "T");
				//setTimeout(function () { location.href = '/my-orders'; }, 10000);
				setTimeout(function () { location.href = '/more-order-information'; }, 10000);
				// sessionStorage.setItem("OrderId", JSON.stringify(f_data.OrderId));
				//$location.url('/more-order-information');
			});
		</script>
	</head>
	<body>
		@model NaamVidya.Models.TransactionSuccessModel
		<div class="business-name-section">
			<div class="container">
				<div class="row mt-5 mb-5">
					<div class="col s12 l12">
						<div class="card z-depth-2 mb-5">
							<div class=" bg-dark lead">
								<img src="images/banner/logo-banner.jpg" alt="" class="card-img-top" />
								@*<p style="text-align:center;">Give us following details all of the following required fields.,</p>*@
							</div>
							<form style="background : #ffffff;" method="post" action="@Url.Action("MakePayUMoneyPayment", "Routes")">
							<div class="order-form" data-ng-show="dvbasictl">
								<div class="form-group row">
									<div class="col l12 m12 s12">
										<center>
											<b style="color:#d70a61;margin-top:20px;font-size:20px;">Your transaction completed successfully with transaction reference no. : @ViewBag.PaymentId   and invoice no : @ViewBag.TrasactionId</b>
											@*<strong>Give us the following detail.Trans_Id</strong>*@
											<br/>
											<b style="color:#d70a61;margin-top:20px;font-size:18px;">Please wait while we redirect you to your Order Page.</b><br/>
											<br/>
											<p style="text-align:center;">Thank you for placing your order.<br/>
												A name specialist will connect with you shortly to process your order.<br />
											In the interim, you can review <a href="https://www.naamvidya.com/how-it-works">How it Works</a> section for more details.</p>
										</center>
									</div>
								</div>
							</div>
							<div class="order-form" data-ng-show="dvproductdtl"></div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>