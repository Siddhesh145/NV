<script>
    (function ($) {
        const actions = {
            "PREVIOUS": "previous",
            "NEXT": "next"
        };

        const total = $(".modal-page").length;

        $(".modal-body").on("click", "a[data-action]", function (e) {
            e.preventDefault();

            var $target = $(e.target),
                current = parseInt($(".modal-pages").attr("data-current")),
                $currentPage = $(".modal-page:nth-child(" + current + ")"),
                offset = 0,
                action = $target.attr("data-action");

            switch (action) {
                case actions.PREVIOUS:
                    if (current > 1) {
                        offset = current - 1;
                    }
                    break;
                default:
                    if (current < total) {
                        offset = current + 1;
                    }
                    break;
            }

            var transformPercentage = (offset - 1) * -100

            $(".modal-page:nth-child(" + current + ")").removeClass("in");
            $(".modal-page:nth-child(" + offset + ")").addClass("in");

            $(".modal-pages")
                .attr("data-current", offset)
                .css("transform", "translate3d(" + transformPercentage + "%, 0, 0)");

            $("#myModal").on("hidden.bs.modal", function (e) {
                $(".modal-pages").attr("data-current", 1).removeAttr("style");
            });
        });

        $("#myModal").modal("show");
    })(jQuery);
</script>
